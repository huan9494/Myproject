
 <!DOCTYPE html>
<html>
  <head>
    <title>Myproject</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container-fluid">
        <%= render 'shared/header' %>

        <!-- sidebar -->

    <div class="row">
        <div class="col-md-2 col-sm-2">
            <ul class="nav nav-pills nav-stacked">
                <li class="active"><a href="<%= root_path %>"><i class="fa fa-home fa-fw"></i>Home</a></li>
                <li><a href="<%= admin_path %>"><i class="fa fa-list-alt fa-fw"></i>Courses</a></li>
                <li><a href="#"><i class="fa fa-file-o fa-fw"></i>Users</a></li>
                <li><a href="#"><i class="fa fa-bar-chart-o fa-fw"></i>Lessons</a></li>
            </ul>
        </div>

        <!-- end of sidebar -->


        <div class="col-md-10 col-sm-10">
            <div id = "course-list-body">
                <%= yield %>
            </div>
        </div>
    </div>

        <%= render 'shared/footer' %>
    </div>
  </body>
</html>
